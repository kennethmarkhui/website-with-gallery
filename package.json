{
  "name": "website-with-gallery",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "db:generate": "dotenv -e .env.local -- npx prisma generate",
    "db:studio": "dotenv -e .env.local -- npx prisma studio",
    "db:migrate": "dotenv -e .env.local -- npx prisma migrate deploy",
    "db:migrate-dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "db:pull": "dotenv -e .env.local -- npx prisma db pull",
    "db:push": "dotenv -e .env.local -- npx prisma db push",
    "db:seed": "dotenv -e .env.local -- npx prisma db seed",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^4.13.0",
    "@react-spring/web": "^9.7.2",
    "@tanstack/react-query": "^4.29.5",
    "@tanstack/react-query-devtools": "^4.29.6",
    "@use-gesture/react": "^10.2.26",
    "cloudinary": "^1.36.4",
    "clsx": "^1.2.1",
    "formidable": "^2.1.1",
    "next": "^13.4.1",
    "next-auth": "^4.22.1",
    "next-intl": "^2.13.2",
    "nodemailer": "^6.9.1",
    "nprogress": "^0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "react-photo-album": "^2.2.0",
    "react-use": "^17.4.0",
    "tailwind-merge": "^1.12.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/formidable": "^2.0.5",
    "@types/node": "^20.1.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "prisma": "^4.13.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
