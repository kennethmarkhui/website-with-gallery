    "@prisma/client": "^5.1.1",
    "@react-spring/web": "^9.7.3",
    "@tanstack/react-query": "^4.32.6",
    "@tanstack/react-query-devtools": "^4.32.6",
    "@tanstack/react-table": "^8.9.3",
    "@use-gesture/react": "^10.2.27",
    "cloudinary": "^1.40.0",
    "clsx": "^2.0.0",
    "formidable": "^3.5.0",
    "negotiator": "^0.6.3",
    "next": "^13.4.13",
    "next-auth": "^4.23.0",
    "next-intl": "^2.19.1",
